{
	"basedir":"APP_DIR",
	"properties":{
		"profiles.dir":"xipki/ca-setup/certprofile",
		"token.type":"pkcs12",
		"ca.signerConf":"algo=SHA256withRSA,password=1234,keystore=file:xipki/ca-setup/keycerts/myca1.p12"
	},
	"signers":[],
	"keypairGens":[],
	"requestors":[{
		"name":"requestor1",
		"type":"cert",
		"binaryConf":{
			"file":"xipki/keycerts/tlskeys/client/tls-client-cert.der"
		}
	}],
	"publishers":[
//	  {
//	    "name":"ocsp-publisher",
//	    "type":"ocsp",
//	    "conf":{
//	        "value":"datasource=ocsp,publish.goodcerts=true"
//	    }
//    }
	],
	"profiles":[{
		"name":"rootca",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-rootca.json"
		}
	},{
		"name":"scep",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-scep.json"
		}
	},{
		"name":"ocsp",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-ocsp.json"
		}
	},{
		"name":"smime",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-smime.json"
		}
	},{
		"name":"tls",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-tls.json"
		}
	},{
		"name":"tls-c",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-tls-c.json"
		}
	},{
		"name":"tls-sm2",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-tls-sm2.json"
		}
	},{
		"name":"tls-sm2-enc",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-tls-sm2-enc.json"
		}
	}],
	"cas":[{
		"name":"myca1",
		"aliases":[
			"myca"
		],
		"caInfo":{
			"caUris":{
				"crlUris":[
					"http://ca.myorg.org/crl/myca1"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/"
				]
			},
			"keypairGenNames": ["software"],
			"crlControl":{
				"interval.hours": 24,
				"fullcrl.intervals":7,
				"overlap":"3d",
				"interval.time":"02:00"
			},
			"expirationPeriod":365,
			"genSelfIssued":{
				"subject":"C=DE,O=myorg,CN=myca1",
				"profile":"rootca",
				"serialNumber":"1"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"10y",
			"nextCrlNo":2,
			"permissions":["all"],
			"saveCert":true,
			"saveKeypair":false,
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":20,
			"status":"active",
			"validityMode":"cutoff"
		},
		"profiles":[
			"ocsp",
			"scep",
			"smime",
			"tls",
			"tls-c",
			"tls-sm2",
			"tls-sm2-enc"
		],
		"publishers":[
//	        "ocsp-publisher"
		],
		"requestors":[{
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

